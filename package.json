{
  "name": "napi-rs-docs",
  "private": true,
  "version": "1.0.0",
  "description": "Website for https://napi.rs",
  "author": "lynweklm@gmail.com",
  "license": "MIT",
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "dependencies": {
    "@mdx-js/react": "^2.1.1",
    "lodash-es": "^4.17.21",
    "markdown-to-jsx": "^7.1.7",
    "next": "12.1.5",
    "nextra": "2.0.0-alpha.49",
    "nextra-theme-docs": "2.0.0-alpha.50",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.24",
    "@types/react": "^18.0.5",
    "cross-env": "^7.0.3",
    "husky": "7",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.3",
    "vercel": "^24.1.0"
  },
  "scripts": {
    "dev": "next",
    "start": "next start",
    "build": "next build",
    "build:en": "next build",
    "build:cn": "cross-env LOCALE=cn next build",
    "format": "prettier -w .",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "*.@(js|jsx|yml|yaml|json|md|mdx)": [
      "prettier --write"
    ]
  },
  "packageManager": "yarn@3.2.0"
}
